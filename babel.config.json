{
    "presets": [
        "razzle/babel", 
        ["@babel/preset-env", { "modules": false }],
        "@babel/preset-react"
    ],
    "plugins": [
        "dynamic-import-node",
        "react-loadable/babel",
        "add-module-exports",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-regenerator",
        "@babel/plugin-syntax-dynamic-import"
    ]
}
